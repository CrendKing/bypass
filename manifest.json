{

  "description": "Bypasses tracking host and redirect to the URL linked in the query parameter",
  "manifest_version": 2,
  "name": "Bypass",
  "version": "1.0",
  "homepage_url": "https://github.com/CrendKing/bypass",
  "icons": {
    "48": "icons/bypass-48.png"
  },
  "permissions": ["tabs"],

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "48": "icons/bypass-48.png"
    }
  }
}
